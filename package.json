{
  "name": "mk-ts-app",
  "version": "1.0.0",
  "author": "neilcochran <dev.neil.cochran@pm.me>",
  "description": "Streamline your Typescript/Node project set up with mk-ts-app. Spend time coding, not setting up your project! This interactive command line app will walk you through a complete project set up in seconds!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neilcochran/mk-ts-app.git"
  },
  "license": "MIT",
  "keywords": [
    "generate",
    "project",
    "typescript project",
    "node project"
  ],
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "lint": "eslint --fix --ext .ts \"src/**/*\"",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/inquirer": "^8.2.1",
    "@types/is-valid-path": "^0.1.0",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "inquirer": "^8.2.4",
    "is-valid-path": "^0.1.1",
    "command-exists": "^1.2.9"
  },
  "files": [
    "dist/**/*",
    "assets/**/*"
  ]
}
